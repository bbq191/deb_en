{
  "update.showReleaseNotes": false,
  "search.followSymlinks": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": -1,
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "workbench.editor.navigationScope": "editorGroup",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "readme",
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "[Tokyo Night Storm]": {
      "foreground": "#959cbd",
      "panelTitle.activeBorder": "#3d59a1",
      "panelTitle.activeForeground": "#bdc7f0",
      "panelTitle.inactiveForeground": "#959cbd",
      "tab.activeForeground": "#bdc7f0",
      "tab.inactiveForeground": "#959cbd",
      "breadcrumb.foreground": "#959cbd",
      "breadcrumb.focusForeground": "#bdc7f0",
      "breadcrumb.activeSelectionForeground": "#bdc7f0",
      "statusBar.foreground": "#bdc7f0",
      "list.focusForeground": "#bdc7f0",
      "list.hoverForeground": "#bdc7f0",
      "list.activeSelectionForeground": "#bdc7f0",
      "list.inactiveSelectionForeground": "#bdc7f0",
      "list.inactiveSelectionBackground": "#202330",
      "sideBar.foreground": "#959cbd",
      "dropdown.foreground": "#959cbd",
      "menu.foreground": "#bdc7f0",
      "menubar.selectionForeground": "#bdc7f0",
      "input.foreground": "#959cbd",
      "input.placeholderForeground": "#959cbd",
      "activityBar.foreground": "#bdc7f0",
      "activityBar.inactiveForeground": "#787c99",
      "gitDecoration.ignoredResourceForeground": "#696d87",
      "editorCodeLens.foreground": "#7982a9", // Preferred hex color
      "window.activeBorder": "#ff0000",
      "window.inactiveBorder": "#0000ff"
    }
  },
  "zenMode.hideLineNumbers": false,
  "terminal.integrated.fontFamily": "'Sarasa Term SC','Source Han Sans SC','Symbols Nerd Font Mono'",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.external.osxExec": "kitty.app",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.organizeImports": true
  },
  "editor.accessibilitySupport": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "'Sarasa Mono SC','Source Han Sans SC','Symbols Nerd Font Mono'",
  "editor.fontLigatures": "'calt','zero','ss01'", // 参考 https://github.com/JetBrains/JetBrainsMono/wiki/OpenType-features
  "editor.fontSize": 17,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.dragAndDrop": false,
  "editor.suggestFontSize": 17,
  "editor.tokenColorCustomizations": {
    "[Tokyo Night Storm]": {
      "textMateRules": [
        {
          "scope": [
            "comment keyword.codetag.notation",
            "comment.block.documentation keyword",
            "comment.block.documentation storage.type.class"
          ],
          "settings": {
            "foreground": "#bb9af7"
          }
        },
        {
          "scope": ["comment.block.documentation entity.name.type.instance"],
          "settings": {
            "foreground": "#73daca",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "comment.block.documentation entity.name.type punctuation.definition.bracket"
          ],
          "settings": {
            "foreground": "#bb9af7"
          }
        },
        {
          "scope": ["comment.block.documentation variable"],
          "settings": {
            "foreground": "#e0af68",
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "editor.scrollBeyondLastLine": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "files.exclude": {
    "**/node_modules": true
  },
  "prettier.semi": false,
  "prettier.singleAttributePerLine": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "projectManager.git.baseFolders": ["$home/Workspace"],
  "projectManager.sortList": "Recent",
  "debug.internalConsoleOptions": "neverOpen",
  "debug.console.fontSize": 15,
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.openDebug": "neverOpen",
  "sortJSON.orderOverride": [
    "name",
    "version",
    "main",
    "module",
    "types",
    "typings",
    "files",
    "publishConfig",
    "repository",
    "scripts",
    "prefix",
    "description",
    "body"
  ],
  "sortJSON.orderUnderride": [
    "resolutions",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "cSpell.userWords"
  ],
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "errorLens.fontStyleItalic": true,
  "git.openRepositoryInParentFolders": "never",
  "git.autorefresh": false,
  "lldb.library": "/Applications/Xcode-beta.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "bazel.buildifierExecutable": "/Users/afu/.local/bin/buildifier",
  "bazel.buildifierFixOnFormat": true,
  "bazel.enableCodeLens": true,
  "bazel.executable": "/opt/homebrew/bin/bazel",
  "crates.compatibleDecorator": "✔",
  "crates.errorDecorator": "！",
  "crates.incompatibleDecorator": "X ${version}",
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.alignEntries": true,
  "evenBetterToml.formatter.columnWidth": 80,
  "path-intellisense.showHiddenFiles": true,
  "rust-analyzer.check.command": "clippy",
  "cmake.configureOnOpen": true,
  "cmake.options.statusBarVisibility": "visible",
  "cmake.cmakePath": "/opt/homebrew/opt/cmake/bin/cmake",
  "cmake.configureArgs": ["-DCMAKE_VERBOSE_MAKEFILE=ON"],
  "cmake.showOptionsMovedNotification": false,
  "clangd.path": "/opt/homebrew/opt/llvm/bin/clangd",
  "java.configuration.maven.userSettings": "/Users/afu/.local/share/sdkman/candidates/maven/current/conf/settings.xml",
  "java.configuration.maven.globalSettings": "",
  "git.ignoreLegacyWarning": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "fixme",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "lldb.launch.terminal": "integrated",
  "lldb.launch.sourceLanguages": ["rust"],
  // "lldb.launch.initCommands": [
  //   "settings set target.x86-disassembly-flavor att",
  //   "settings set target.skip-prologue false"
  // ],
  "vim.foldfix": true,
  "vim.highlightedyank.color": "rgba(230, 97, 89, 0.7)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["g", "g", "V", "G"],
      "before": ["<c-a>"]
    },
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["leader", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["leader", "x"],
      "commands": [":x!"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    }
  ],
  "workbench.colorTheme": "Everforest Dark",
  "everforest.darkContrast": "hard",
  "everforest.italicKeywords": true,
  "everforest.lightContrast": "hard",
  "everforest.lightSelection": "green",
  "everforest.darkCursor": "red",
  "everforest.darkWorkbench": "flat",
  "everforest.lightCursor": "green",
  "everforest.lightWorkbench": "flat",
  "everforest.darkSelection": "red"
}
